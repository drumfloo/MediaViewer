            ],
            "tsConfig": "projects/zam-viewer/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/zam-viewer/src/favicon.ico",
              "projects/zam-viewer/src/assets"
            ],
            "styles": [
              "projects/zam-viewer/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
