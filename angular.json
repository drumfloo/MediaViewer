            "index": "projects/zam-viewer/src/viewer_index.html",
            "main": "projects/zam-viewer/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/zam-viewer/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/zam-viewer/src/favicon.ico",
              "projects/zam-viewer/src/assets"
            ],
            "styles": [
              "projects/zam-viewer/src/styles.scss",
              "./node_modules/@videogular/ngx-videogular/fonts/videogular.css",
              "src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
